<% include inc/top.ejs %>
<div class="nycg-main nycg-middle nycg-emphasis">
<h1><span><% if (typeof pageHeader !== 'undefined') { %><%= pageHeader %><% } %></span></h1>
</div>
<div class="nycg-main nycg-bottom">
<%
    if (venue) {
%>
    <article class="nycg-layout nycg-layout-50">
        <div class="nycg-col nycg-col-1">
            <% if (venue.bio != "") { %>
            <%- venue.bio %>
            <% } else { %>
            <h3>No bio yet :( </h3>
            <% } %>
        </div>
        <aside class="nycg-col nycg-col-2">
            <figure><img src="<%= venue.pic_cdn %>" alt="<%= venue.displayName %>" /></figure>
            <% if (venue.link != "") { %>
            <h2><span>Website</span></h2>
            <a href="<%= venue.link %>" class="nycg-external" target="_blank"><%= venue.link %></a>
            <% } %>
            <% if (venue.address != "") { %>
            <h2><span>Address</span></h2>
            <p><strong><%= venue.displayName %></strong><br/>
                <%= venue.address %><br/>
            </p>
            <% } %>
            <% if (venue.hours != "") { %>
            <h2><span>Hours</span></h2>
            <p><%= venue.hours %></p>
            <% } %>
            <h2><span>Share This Page</span></h2>
            <div>
                <span class='st_facebook_large' displayText='Facebook'></span>
                <span class='st_googleplus_large' displayText='Google +'></span>
                <span class='st_twitter_large' displayText='Tweet'></span>
                <span class='st_linkedin_large' displayText='LinkedIn'></span>
                <span class='st_pinterest_large' displayText='Pinterest'></span>
            </div>
        </aside>
    </article>
    <article class="nycg-layout nycg-layout-25 nycg-gallery">
        <% include inc/list-venue-photos.ejs %>
    </article>
    <article class="nycg-layout nycg-layout-25 nycg-latest-events">
        <% include inc/venue-latest-events.ejs %>
    </article>
<%
    } else {
%>
    <h1>No artist found.</h1>
<%
    }
%>
    <% include inc/footer.ejs %>
</div>
<% include inc/bottom.ejs %>